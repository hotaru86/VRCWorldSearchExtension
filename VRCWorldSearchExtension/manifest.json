{
    "manifest_version": 3,
    "name": "VRC World Search Extension",
    "version": "1.0",
    "description": "右クリックで選択したテキストをVRChatで検索します。",
    "permissions": ["contextMenus", "tabs"],
    "background": {
        "service_worker": "background.js"
    },
    "icons": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
    }
}